{
  "manifest_version": 3,
  "name": "Google AI Results Blocker",
  "description": "Blocks Google AI Overview. Because it sucks and I don't want to see it.",
  "version": "1.0",
  "icons": {
    "16": "icons/no_ai16.png",
    "32": "icons/no_ai32.png",
    "48": "icons/no_ai48.png",
    "128": "icons/no_ai128.png"
  },
  "content_scripts": [
    {
      "js": ["logic.js"],
      "matches": [
        "https://www.google.com/search*"
      ]
    }
  ]
}